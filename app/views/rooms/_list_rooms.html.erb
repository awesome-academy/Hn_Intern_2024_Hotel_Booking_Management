<div class="row g-4" id="show-list-rooms">
  <% if @room_types != nil && @room_types.length > 0 %>
    <%= render partial: "room", collection: @room_types, as: :item, locals: {check_in:, check_out:, num_guest:} %>
    <%== pagy_bootstrap_nav @pagy if @pagy.pages > 1 %>
  <% else %>
    <h3 class="text-center text-secondary">
      <%= t ".not_found" %>
    </h3>
  <% end %>
</div>
